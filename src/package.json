{
  "name": "prisma-starter",
  "scripts": {
    "start": "npm run start:docker:all",
    "build": "npm run docker:rebuild:all",
    "start:docker:db": "PRISMA_ENDPOINT=http://localhost:4466 npm-run-all --serial docker:compose:db dev",
    "start:docker:all": "PRISMA_ENDPOINT=http://prisma:4466 npm run docker:compose:all",
    "start:docker:entrypoint": "npm run start:vanilla",
    "start:vanilla": "babel-node service/index.js",
    "playground": "graphql playground",
    "dev": "npm-run-all --parallel dev:liveReload playground",
    "dev:liveReload": "nodemon --watch service/index.js --exec 'babel-node service/index.js'",
    "docker:compose:db": "docker-compose -f database/docker-compose.db.yml up -d",
    "docker:compose:all": "docker-compose -f docker-compose.all.yml up -d",
    "docker:rebuild:db": "docker-compose -f database/docker-compose.db.yml up -d --force-recreate --build",
    "docker:rebuild:all": "docker-compose -f docker-compose.all.yml up -d --force-recreate --build",
    "test": "npm run test:liveReload",
    "test:vanilla": "ava",
    "test:liveReload": "ava --watch"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.13.1",
    "jsonwebtoken": "8.2.1",
    "prisma-binding": "1.5.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.39",
    "@babel/node": "^7.0.0-beta.39",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/register": "^7.0.0",
    "ava": "^0.25.0",
    "babel-plugin-dotenv": "^0.1.1",
    "dotenv": "5.0.1",
    "graphql-cli": "2.16.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "4.1.3",
    "npm-starter-sample-module": "^0.0.2",
    "prisma": "1.8.3"
  },
  "ava": {
    "files": [
      "service/tests/**.js"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "verbose": true
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "9.3.0"
          }
        }
      ]
    ],
    "plugins": [
      [
        "babel-plugin-dotenv",
        {
          "replacedModuleName": "babel-dotenv"
        }
      ]
    ]
  }
}
